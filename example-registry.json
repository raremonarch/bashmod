{
  "version": "1.0",
  "modules": [
    {
      "id": "git-helpers",
      "description": "Git aliases and clone helpers for multiple remotes",
      "version": "1.0.0",
      "url": "https://raw.githubusercontent.com/user/bashrc-modules/main/git.sh",
      "category": "version-control",
      "dependencies": ["ssh-agent"],
      "exports": {
        "aliases": ["gitaddcommit"],
        "functions": ["ssh_load_key_for_url", "clone-eis", "clone-daevski", "git-del-branch"],
        "variables": []
      }
    },
    {
      "id": "docker-utils",
      "description": "Docker cleanup and management functions",
      "version": "1.0.0",
      "url": "https://raw.githubusercontent.com/user/bashrc-modules/main/docker.sh",
      "category": "containers",
      "dependencies": [],
      "exports": {
        "aliases": [],
        "functions": ["docker-nukec", "docker-purge-all"],
        "variables": []
      }
    },
    {
      "id": "ssh-agent",
      "description": "Persistent SSH agent management with key loading",
      "version": "1.0.0",
      "url": "https://raw.githubusercontent.com/user/bashrc-modules/main/ssh-agent.sh",
      "category": "security",
      "dependencies": [],
      "exports": {
        "aliases": [],
        "functions": ["is_key_loaded", "ssh-add-all"],
        "variables": ["SSH_AUTH_SOCK", "SSH_AGENT_PID"]
      }
    },
    {
      "id": "python-dev",
      "description": "Python virtual environment and development helpers",
      "version": "1.0.0",
      "url": "https://raw.githubusercontent.com/user/bashrc-modules/main/python.sh",
      "category": "development",
      "dependencies": [],
      "exports": {
        "aliases": ["py", "venv-activate"],
        "functions": ["mkvenv", "rmvenv"],
        "variables": []
      }
    },
    {
      "id": "aws-helpers",
      "description": "AWS CLI shortcuts and profile management",
      "version": "1.0.0",
      "url": "https://raw.githubusercontent.com/user/bashrc-modules/main/aws.sh",
      "category": "cloud",
      "dependencies": [],
      "exports": {
        "aliases": [],
        "functions": ["aws-profile-switch", "aws-whoami"],
        "variables": ["AWS_PROFILE"]
      }
    }
  ]
}
